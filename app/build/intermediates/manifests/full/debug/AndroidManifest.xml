<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ivancrnogorac.domacizadatak15"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="22" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar" >

        <!-- > Prva i glavna aktivnost < -->
        <activity android:name="com.example.ivancrnogorac.domacizadatak15.aktivnosti.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- > Druga aktivnost < -->
        <activity
            android:name="com.example.ivancrnogorac.domacizadatak15.aktivnosti.SecondActivity"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/second_activity_name" >
        </activity>

        <!-- > Treca aktivnost < -->

    </application>

</manifest>